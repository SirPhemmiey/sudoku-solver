{
  "transform": {
    "^.+\\.(t|j)sx?$": "babel-jest"
  },
  "transformIgnorePatterns": ["<rootDir>/node_modules/", "<rootDir>/build/"],
  "testEnvironment": "node",
  "runner": "groups",
  "rootDir": "source",
  "collectCoverage": true,
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "verbose": true,
  "clearMocks": true
}
